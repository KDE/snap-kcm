#, kde-format
msgid "Operation was cancelled"
msgstr "La operación se ha cancelado"

#: src/snapbackend.cpp:359
#, kde-format
msgid "A bad query was provided"
msgstr "Se ha proporcionado una consulta incorrecta"

#: src/snapbackend.cpp:362
#, kde-format
msgid "A timeout occurred during the request"
msgstr "Se ha agotado el tiempo de espera durante la petición"

#: src/snapbackend.cpp:365
#, kde-format
msgid "The requested snap couldn’t be found"
msgstr "No se ha podido encontrar el paquete snap solicitado"

#: src/snapbackend.cpp:368
#, kde-format
msgid "The requested snap is not in the store"
msgstr "El paquete snap solicitado no está en la tienda"

#: src/snapbackend.cpp:371
#, kde-format
msgid "Authentication was cancelled by the user"
msgstr "El usuario ha cancelado la autenticación"

#: src/snapbackend.cpp:374
#, kde-format
msgid "Snap not compatible with classic mode"
msgstr "Paquete snap no compatible con el modo clásico"

#: src/snapbackend.cpp:377
#, kde-format
msgid "Requested snap revision not available"
msgstr "La revisión del paquete snap solicitada no está disponible"

#: src/snapbackend.cpp:380
#, kde-format
msgid "Requested snap channel not available"
msgstr "El canal snap solicitado no está disponible"

#: src/snapbackend.cpp:383
#, kde-format
msgid "The given snap or directory does not look like a snap"
msgstr ""
"El paquete snap o el directorio que se han indicado no parecen ser un "
"paquete snap"

#: src/snapbackend.cpp:386
#, kde-format
msgid "A hostname failed to resolve during the request"
msgstr "No se ha podido resolver un nombre de máquina durante la solicitud"

#: src/snapbackend.cpp:389
#, kde-format
msgid "A requested configuration option is not set"
msgstr "No se ha definido una opción de configuración solicitada"

#: src/snapbackend.cpp:392
#, kde-format
msgid "The requested app couldn't be found"
msgstr "No se ha podido encontrar la aplicación solicitada"

#: src/snapbackend.cpp:395
#, kde-format
msgid "No snap revision on specified architecture"
msgstr "No hay ninguna revisión de snap para la arquitectura indicada"

#: src/snapbackend.cpp:398
#, kde-format
msgid "The requested operation would conflict with currently ongoing change"
msgstr ""
"La operación solicitada entraría en conflicto con el cambio que se está "
"llevando a cabo"

#: src/snapbackend.cpp:401
#, kde-format
msgid "The requested interface has not changed"
msgstr "La interfaz solicitada no ha cambiado"

#: src/snapbackend.cpp:404
#, kde-format
msgid "Unknown error code"
msgstr "Código de error desconocido"

